{
    "name": "Web Time tracker",
    "version": "1.0.0",
    "description": "Tracks time spent on sites",
    "manifest_version": 3,
    "action":{
        "default_title": "Web Time tracker"
    },
    "minimum_chrome_version": "121",
    "permissions": ["storage", "tabs"],
    "host_permissions": [
        "http://localhost:8080/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}